//Napisz funkcje ktora otrzymuje dwa argumenty nieujemna liczba calkowita n oraz n-elementowa tablice tab elementow typu int.Â 

#include <stdio.h>
#include <stdlib.h>
void wypisz(unsigned int n,int *tab)
{
for(int i=0;i<n;i++)
{
printf("%d",tab[i]);
}
}
void xd(unsigned int n,int tab[])
{
for(int i=0;i<n;i++)
{
tab[i]=0;
}
}
int main()
{
unsigned int n=10;
int tab[10]={5,2,3,4,5,6,8,8,8,1};
wypisz(n,tab);
printf("\n");
xd(n,tab);
wypisz(n,tab);
}
